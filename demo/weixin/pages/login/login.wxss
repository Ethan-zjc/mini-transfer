.navigation {
  position: fixed;
  top: 0;
  z-index: 9999;
  width: 100%;
}

.navigation-content {
  display: flex;
  align-items: center;
}

.navigation-close {
  margin-left: 28rpx;
  width: 30rpx;
  height: 30rpx;
}

.navigation-close image {
  width: 100%;
  height: 100%;
}

.login-logo {
  display: flex;
  align-items: center;
  margin-top: 120rpx;
  margin-bottom: 100rpx;
  padding: 0 32rpx;
  height: 88rpx;
}

.login-logo image {
  width: 88rpx;
  height: 88rpx;
}

.login-logo text {
  margin-left: 32rpx;
  font-size: 48rpx;
  font-weight: 500;
  color: #333;
}

.header-warp {
  position: relative;
  margin-bottom: 80rpx;
  width: 750rpx;
}

.header-warp .logo-wrap {
  position: relative;
  z-index: 10;
  display: flex;
  align-items: center;
  padding-left: 90rpx;
  width: 100%;
  box-sizing: border-box;
}

.header-warp .logo-wrap .logo {
  margin-right: 32rpx;
  width: 88rpx;
  height: 88rpx;
}

.header-warp .logo-wrap .login-text {
  font-size: 48rpx;
  font-family: PingFangSC, PingFangSC-Medium;
  font-weight: 500;
  text-align: center;
  color: #333;
}

.develop {
  padding: 20rpx 90rpx 100rpx;
}

.develop button {
  margin-top: 16rpx;
  min-width: 100%;
  height: 80rpx;
  font-size: 24rpx;
  background: #f1f2f8;
  line-height: 80rpx;
}

.develop .intro {
  font-size: 24rpx;
  text-align: center;
  color: #f43530;
  line-height: 24rpx;
}

.develop .switch {
  color: #442509;
}

.develop .continuRead {
  color: #ff751a;
}

.develop .phone {
  color: #005bad;
}

.develop .logout {
  color: #f43530;
}
