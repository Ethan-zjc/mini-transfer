.find-common-title {
  padding: 32rpx 0;
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 36rpx;
  font-family: PingFangSC-Medium,PingFang SC;
  font-weight: bold;
  color: #333;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  word-break: break-all;
}

.guess-topics {
  box-sizing: border-box;
  width: 750rpx;
  padding: 0 24rpx 32rpx;
  height: auto;
}

.guess-topics .topics-list {
  display: flex;
  height: auto;
  padding-bottom: 20rpx;
}

.guess-topics-content {
  overflow: hidden;
  height: 246rpx;
}
