<view class="guess-topics">
    <view class="find-common-title">{{ list.title }}</view>
    <view class="guess-topics-content">
        <scroll-view scroll-x="true">
            <view class="topics-list">
                <block 
                    wx:for="{{ list.banner_list }}"
                    wx:for-index="ind" 
                    wx:key="uuid"> 
                    <kk-find-topics 
                        height="{{ 156 }}"
                        width="{{ 248 }}"
                        cssName="find-guess-topics"
                        i="{{ ind }}"
                        datas="{{ item }}"
                        listIndex="{{ listIndex }}"
                        moduletype="{{ list.module_type }}"
                        moduleid="{{ list.module_id }}"
                        modulemame="{{ list.title }}"
                        listWrapIndex="{{ newIndex }}"
                        bind:onSubnavTap="subnavTap"
                    />
                </block>
            </view>
        </scroll-view>
    </view>
</view>