@charset "UTF-8";
.kk-find-page {
  width: 100%;
  position: relative;
}

.kk-find-page .subnav {
  position: relative;
  width: 100%;
  height: 143rpx;
  padding-top: 20rpx;
  display: flex;
  align-items: center;
  overflow: hidden;
}

.kk-find-page .subnav .view {
  position: relative;
  width: 368rpx;
  height: 143rpx;
}

.kk-find-page .subnav .view .btn {
  position: absolute;
  left: 0;
  right: 0;
  width: 100%;
  height: 100%;
  z-index: 1000;
}

.kk-find-page .subnav .view .text-icon {
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-between;
  position: absolute;
  left: 0;
  top: 0;
  padding: 49rpx 73rpx;
  width: 100%;
  height: 100%;
  z-index: 1;
}

.kk-find-page .subnav .view .text-icon .text {
  padding-left: 14rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Medium,PingFang SC;
  font-weight: 500;
  color: #333333;
}

.kk-find-page .subnav-new {
  box-sizing: border-box;
  width: 100%;
  padding-top: 44rpx;
  padding-bottom: 28rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.kk-find-page .subnav-new .view-tap {
  flex: 1;
  position: relative;
  display: flex;
  align-items: center;
  justify-content: center;
  height: 96rpx;
}

.kk-find-page .subnav-new .view-tap .btn {
  position: absolute;
  left: 0;
  right: 0;
  width: 100%;
  height: 100%;
  z-index: 1000;
}

.kk-find-page .subnav-new .view-tap .view-tap-bg {
  width: 255rpx;
  height: 144rpx;
}

.kk-find-page .subnav-new .view-tap .middle {
  width: 240rpx;
}

.kk-find-page .subnav-new .view-tap .tab-con {
  position: absolute;
  left: 0;
  top: 0;
  z-index: 1;
  width: 100%;
  height: 96rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.kk-find-page .subnav-new .view-tap .tab-con .text {
  padding-left: 16rpx;
}

.kk-find-page .subnav-new .view-tap .tab-con .red-dot {
  width: 16rpx;
  height: 16rpx;
  border-radius: 50%;
  background: #ff6860;
  position: absolute;
  right: 51rpx;
  top: 24rpx;
  z-index: 1;
}

.kk-find-page .subnav-new .view-tap .last-tab-con {
  width: 230rpx;
}

.kk-find-page .subnav-new .view-tap .last-tab-con .red-dot {
  right: 31rpx;
}

.kk-find-page .subnav-new .tab-kk-icon {
  width: 48rpx;
  height: 48rpx;
}

.kk-find-page .hands-pd-top {
  padding-top: 0rpx;
}

.kk-find-page .six-topics, .kk-find-page .four-topics, .kk-find-page .four-topics-vertical {
  box-sizing: border-box;
  width: 750rpx;
  padding: 0 24rpx 32rpx;
  height: auto;
}

.kk-find-page .six-topics .topics-list, .kk-find-page .four-topics .topics-list, .kk-find-page .four-topics-vertical .topics-list {
  margin: 0 auto;
  box-sizing: border-box;
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  justify-content: space-between;
  width: 702rpx;
  height: auto;
  padding-bottom: 8rpx;
}

.kk-find-page .ranking-topics {
  box-sizing: border-box;
  width: 750rpx;
  height: auto;
  padding: 0 0 32rpx;
}

.kk-find-page .ranking-topics .ranking-top-bg image {
  filter: blur(6rpx);
}

.kk-find-page .ranking-topics .nav-list {
  position: relative;
  padding: 0rpx 24rpx;
  height: 112rpx;
  display: flex;
  align-items: center;
}

.kk-find-page .ranking-topics .nav-list .nav-tap {
  margin-right: 34rpx;
  line-height: 26rpx;
  font-size: 30rpx;
  font-family: PingFangSC-Regular,PingFang SC;
  font-weight: 400;
  color: #666;
  height: 112rpx;
  line-height: 112rpx;
  position: relative;
}

.kk-find-page .ranking-topics .nav-list .nav-tap:after {
  content: '';
  display: none;
  width: 40rpx;
  height: 8rpx;
  border-radius: 4rpx;
  background: #FDE23D;
  position: absolute;
  left: 50%;
  margin-left: -20rpx;
  top: 84rpx;
}

.kk-find-page .ranking-topics .nav-list .active {
  padding-top: 0;
  font-size: 36rpx;
  font-family: PingFangSC-Semibold,PingFang SC;
  font-weight: 600;
  color: #333;
}

.kk-find-page .ranking-topics .nav-list .active:after {
  display: block;
}

.kk-find-page .ranking-topics .nav-list .right-more {
  position: absolute;
  top: 50%;
  right: 24rpx;
  transform: translateY(-50%);
  display: flex;
  align-items: center;
}

.kk-find-page .ranking-topics .nav-list .right-more .text {
  line-height: 26rpx;
  font-size: 26rpx;
  font-family: PingFangSC-Regular,PingFang SC;
  font-weight: 400;
  color: #999999;
}

.kk-find-page .ranking-topics .nav-list .right-more .icon {
  position: relative;
  top: -1rpx;
  width: 24rpx;
  height: 24rpx;
}

.kk-find-page .ranking-topics .topics-list-box {
  position: relative;
  width: 100%;
  height: 830rpx;
}

.kk-find-page .ranking-topics .topics-list {
  position: absolute;
  left: 0;
  top: 0;
  z-index: 1;
  box-sizing: border-box;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  padding: 24rpx 24rpx 110rpx;
  width: 750rpx;
  height: 830rpx;
  background: linear-gradient(184deg, rgba(0, 64, 114, 0.3) 0%, rgba(0, 6, 75, 0.7) 100%) rgba(0, 0, 0, 0.4);
}

.kk-find-page .ranking-topics .topics-more {
  position: absolute;
  left: 24rpx;
  bottom: 30rpx;
  width: 702rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  background: rgba(255, 255, 255, 0.1);
  border-radius: 12rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #FFFFFF;
  z-index: 3;
}

.kk-find-page .no-pd-4 {
  padding-bottom: 0;
}

.kk-find-page .label-topics {
  width: 100%;
  padding: 0 24rpx 12rpx;
  box-sizing: border-box;
}

.kk-find-page .label-topics .labels-view {
  width: 100%;
  max-height: 117px;
  display: flex;
  flex-wrap: wrap;
  overflow: hidden;
}

.kk-find-page .label-topics .label-text {
  height: 29px;
  padding: 0px 8px;
  box-sizing: border-box;
  line-height: 29px;
  background: #F7F7F8;
  border-radius: 4rpx;
  margin-right: 16rpx;
  font-size: 30rpx;
  color: #333;
  width: auto;
  max-width: 702rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-bottom: 10px;
}

.kk-find-page .special-banner {
  margin: 0 24rpx;
  box-sizing: border-box;
  position: relative;
}

.kk-find-page .special-banner .banner-img {
  position: relative;
  width: 100%;
}

.kk-find-topics {
  box-sizing: border-box;
  height: auto;
}

.kk-find-topics .cover {
  position: relative;
  box-sizing: border-box;
  width: 100%;
  border-radius: 4rpx;
  overflow: hidden;
}

.kk-find-topics .cover image {
  border-radius: 4rpx;
}

.kk-find-topics .cover .bottom {
  box-sizing: border-box;
  padding: 20rpx 12rpx 14rpx;
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 56rpx;
  background: linear-gradient(180deg, transparent 0%, rgba(0, 0, 0, 0.5) 100%);
  border-radius: 0px 0px 4rpx 4rpx;
  font-size: 22rpx;
  font-family: PingFangSC-Regular,PingFang SC;
  font-weight: 400;
  color: white;
  text-shadow: 0px 1px 8rpx rgba(0, 0, 0, 0.24);
  text-align: right;
}

.kk-find-topics .cover .bottom view {
  display: inline-block;
  max-width: 36%;
  padding-left: 8rpx;
  font-size: 22rpx;
  font-family: PingFangSC-Regular,PingFang SC;
  font-weight: 400;
  color: white;
  overflow: hidden;
}

.kk-find-topics .cover .ranking-icon {
  display: none;
}

.kk-find-topics .topic-name {
  box-sizing: border-box;
  width: 100%;
  padding: 0 4rpx;
  line-height: 30rpx;
  height: 30rpx;
  font-size: 30rpx;
  font-family: PingFangSC-Regular,PingFang SC;
  font-weight: 400;
  color: #333333;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  word-break: break-all;
}

.kk-find-topics .topic-des {
  padding: 0 4rpx;
  height: 24rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular,PingFang SC;
  font-weight: 400;
  color: #999;
  line-height: 24rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  word-break: break-all;
}

.find-six-topics {
  padding-bottom: 24rpx;
  width: 234rpx;
}

.find-six-topics .cover {
  margin-bottom: 20rpx;
}

.find-six-topics .topic-name {
  margin-bottom: 14rpx;
}

.find-six-topics .topic-des {
  height: 24rpx;
  line-height: 24rpx;
}

.find-six-topics .topic-des view {
  display: inline-block;
  max-width: 33%;
  padding-right: 8rpx;
  font-size: 22rpx;
  font-family: PingFangSC-Regular,PingFang SC;
  font-weight: 400;
  color: #999;
  overflow: hidden;
  white-space: nowrap;
  overflow: hidden;
}

.find-four-topics {
  padding-bottom: 32rpx;
  width: 355rpx;
}

.find-four-topics .cover {
  margin-bottom: 20rpx;
}

.find-four-topics .topic-name {
  margin-bottom: 14rpx;
}

.find-ranking-topics {
  padding-bottom: 28rpx;
  width: 234rpx;
}

.find-ranking-topics .cover {
  position: relative;
  margin-bottom: 16rpx;
}

.find-ranking-topics .cover .ranking-icon {
  position: absolute;
  left: 0;
  top: 0;
  display: block;
  width: 50rpx;
  height: 50rpx;
  z-index: 1;
  border-radius: 4rpx 0px 14px 0px;
}

.find-ranking-topics .cover .ranking-icon .icon {
  position: relative;
  left: -2rpx;
  top: -2rpx;
}

.find-ranking-topics .cover .ranking-icon .text {
  box-sizing: border-box;
  position: absolute;
  top: 0;
  left: 0;
  padding-top: 6rpx;
  padding-left: 12rpx;
  width: 47rpx;
  line-height: 32rpx;
  font-size: 28rpx;
  font-family: FZDHTJW--GB1-0,FZDHTJW--GB1;
  font-weight: normal;
  color: white;
}

.find-ranking-topics .cover .ranking-icon .text-all {
  padding-left: 0rpx;
  text-align: center;
}

.find-ranking-topics .topic-name {
  margin-bottom: 12rpx;
  color: #fff;
}

.find-ranking-topics .topic-des {
  display: flex;
  align-items: center;
  justify-items: center;
}

.find-ranking-topics .topic-des .text {
  padding-left: 4rpx;
  line-height: 24rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular,PingFang SC;
  font-weight: 400;
  color: white;
}

.kk-find-topic-list-btn {
  display: flex;
  align-items: center;
}

.kk-find-topic-list-btn .left {
  margin-right: 10rpx;
}

.kk-find-topic-list-btn .left, .kk-find-topic-list-btn .right {
  flex: 1;
  height: 80rpx;
  line-height: 80rpx;
  background: #F7F7F8;
  border-radius: 8rpx;
  text-align: center;
  font-size: 24rpx;
  font-family: PingFangSC-Regular,PingFang SC;
  font-weight: 400;
  color: #666;
  border-color: #fff;
}

.kk-find-topic-list-btn .left::after, .kk-find-topic-list-btn .right::after {
  border: none;
}

.find-search {
  position: -webkit-sticky;
  position: sticky;
  top: 0px;
  z-index: 10999;
  background: #fff;
}

.find-search-active {
  background: none;
}

.countdown {
  display: inline-block;
}

.countdown-number {
  width: 32rpx;
  height: 30rpx;
  line-height: 30rpx;
  text-align: center;
  background: #F9F6FF;
  border-radius: 4rpx;
  font-size: 20rpx;
  color: #5B29F4;
}

.countdown-space-first::before {
  font-size: 20rpx;
  color: #A8A1B6;
  content: '天';
  margin: 0 5rpx;
  font-weight: normal;
}

.countdown-space-second::before {
  font-size: 20rpx;
  color: #A8A1B6;
  content: '时';
  margin: 0 5rpx;
  font-weight: normal;
}

.countdown-space-third::before {
  font-size: 20rpx;
  color: #A8A1B6;
  content: '分';
  margin: 0 5rpx;
  font-weight: normal;
}

.find-common-title {
  padding: 32rpx 0;
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 36rpx;
  font-family: PingFangSC-Medium,PingFang SC;
  font-weight: bold;
  color: #333;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  word-break: break-all;
}

.margin-top {
  margin-top: 16rpx;
}
