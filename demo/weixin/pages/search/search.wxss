.container {
  color: #442509;
}

.search-wrap {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 100;
  padding: 32rpx 0;
  width: 100%;
  height: 56rpx;
  background: #fff;
}

.search-wrap .search-view {
  position: relative;
  display: flex;
  justify-content: flex-start;
  height: 64rpx;
  border-radius: 64rpx;
  background: #f5f5f5;
}

.search-wrap .search-find {
  margin: 0 auto;
  width: 718rpx;
}

.search-wrap .search-sear {
  margin-left: 24rpx;
  width: 600rpx;
}

.search-wrap .search-blot {
  float: left;
  width: 544rpx;
  border-top-right-radius: 0;
  border-bottom-right-radius: 0;
}

.search-view image {
  margin: 14rpx 12rpx 0 20rpx;
  width: 36rpx;
  min-width: 36rpx;
  height: 36rpx;
}

.search-wrap input {
  position: relative;
  z-index: 0;
  width: 100%;
  height: 64rpx;
  line-height: 64rpx;
  font-size: 26rpx;
  color: #999;
}

.search-wrap .placeholder {
  color: #aaa;
}

.search-wrap .btn-delete {
  position: relative;
  float: left;
  width: 64rpx;
  height: 64rpx;
  border-top-right-radius: 64rpx;
  border-bottom-right-radius: 64rpx;
  background: #f3f5f5;
}

.search-wrap .btn-delete image {
  position: relative;
  z-index: 1;
  margin: 16rpx 0rpx 0 10rpx;
  width: 32rpx;
  height: 32rpx;
}

.search-wrap .btn-cancel {
  position: absolute;
  top: 32rpx;
  right: 32rpx;
  width: 96rpx;
  height: 64rpx;
  font-size: 30rpx;
  font-weight: 400;
  text-align: right;
  color: #333;
  line-height: 64rpx;
}

.search-mask,
.search-content {
  position: absolute;
  padding-top: 128rpx;
  width: 100%;
  height: 100%;
  background: #fff;
  box-sizing: border-box;
}

.search-mask {
  z-index: 10;
}

.search-mask .hits-wrap {
  padding: 0 40rpx;
}

.search-mask .hits-btn {
  padding: 10rpx 0;
  height: 40rpx;
  font-size: 24rpx;
  color: #4a90e2;
  line-height: 40rpx;
}

.search-mask .hits-item {
  overflow: hidden;
  height: 80rpx;
  font-size: 28rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  color: #333;
  line-height: 80rpx;
}

.search-content {
  z-index: 1;
}

.search-tit {
  display: flex;
  justify-content: space-between;
  margin: 24rpx 0;
  padding: 0rpx 24rpx;
  height: 50rpx;
  font-size: 36rpx;
  font-weight: 500;
  color: #333;
  line-height: 50rpx;
}

.search-tit .ico-clear {
  margin-top: 13rpx;
  width: 24rpx;
  height: 24rpx;
}

.search-tit .ico-refresh {
  margin-top: 13rpx;
  width: 24rpx;
  height: 24rpx;
}

.search-hot {
  display: flex;
  justify-content: flex-start;
  padding: 10rpx 40rpx;
  flex-wrap: wrap;
}

.search-hot view {
  margin: 0rpx 15rpx 15rpx 0rpx;
  padding: 12rpx 14rpx;
  font-size: 26rpx;
  border: 1rpx solid #e6e6e6;
  border-radius: 100rpx;
  text-align: center;
  box-sizing: border-box;
}

.search-hot .tag {
  display: inline-block;
  margin: -5rpx 0 0 8rpx;
  width: 46rpx;
  height: 26rpx;
  font-size: 18rpx;
  border-radius: 6rpx;
  text-align: center;
  color: #fff;
  background: #999;
  line-height: 26rpx;
  vertical-align: middle;
}

.search-his {
  padding: 0 40rpx;
}

.history-item {
  display: flex;
  justify-content: space-between;
  line-height: 88rpx;
  border-bottom: 1rpx solid #e6e6e6;
}

.history-item text {
  overflow: hidden;
  margin-left: 14rpx;
  font-size: 28rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  color: #333;
}

.history-item view:nth-child(1) {
  overflow: hidden;
  width: 100%;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.history-item view:nth-child(2) {
  width: 44rpx;
  text-align: right;
}

.history-item image {
  vertical-align: middle;
}

.search-list {
  height: 100%;
}

.search-mask .topic {
  padding: 24rpx;
  background: #fff;
}

.topic-list .topic {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.topic-list .pic {
  overflow: hidden;
  width: 224rpx;
  border-radius: 16rpx;
}

.topic-list .txt {
  position: relative;
  display: flex;
  overflow: hidden;
  padding: 0 0 0 24rpx;
  min-width: 0;
  flex-direction: column;
  flex: 1;
}

.topic-list .video .btn-list {
  display: flex;
  padding: 0 24rpx;
}

.topic-list .video .btn-list text {
  line-height: 72rpx;
  font-size: 30rpx;
  font-weight: 500;
}

.topic-list .video .btn-list .btn {
  width: 343rpx;
  height: 72rpx;
  border-radius: 44rpx;
  text-align: center;
  background: rgba(0, 0, 0, 0.05);
}

.topic-list .video .btn-list .like {
  color: #222;
}

.topic-list .video .btn-list .liked {
  color: #ccc;
}

.topic-list .video .btn-list .comic {
  margin-left: 16rpx;
  background: #FFE120;
}

.topic-list .video .manju {
  display: flex;
  align-items: center;
  margin: 24rpx;
  padding: 0 24rpx;
  height: 88rpx;
  border-radius: 6px;
  background: #FAFAFA;
}

.topic-list .video .manju .icon {
  padding: 2px 4px;
  font-size: 22rpx;
  font-weight: 500;
  border-radius: 6rpx;
  background: #FFE120;
}

.topic-list .video .manju .title {
  overflow: hidden;
  margin-left: 12rpx;
  width: 200px;
  font-size: 30rpx;
  font-weight: 500;
  text-overflow: ellipsis;
  white-space: nowrap;
  color: #222;
}

.topic-list .video .manju .right {
  display: flex;
  align-items: center;
  margin-left: auto;
  font-size: 26rpx;
  color: #999;
}

.topic-list .video .manju .right .play {
  margin-right: 4rpx;
  width: 28rpx;
  height: 28rpx;
}

.topic-list .video .manju .right .arrow {
  width: 24rpx;
  height: 24rpx;
}

.topic-list .top {
  display: flex;
  height: 56rpx;
}

.topic-list .top .tit {
  display: block;
  overflow: hidden;
  margin-right: 40rpx;
  width: 100%;
  min-width: 0;
  font-size: 34rpx;
  font-weight: 500;
  text-overflow: ellipsis;
  white-space: nowrap;
  color: #222;
  line-height: 56rpx;
  flex: 1;
}

.topic-list .btn {
  position: relative;
  min-width: 96rpx;
}

.topic-list .btn .like {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 132rpx;
  height: 56rpx;
  font-size: 26rpx;
  font-weight: 400;
  border: 2rpx solid #e6e6e6;
  border-radius: 28rpx;
  color: #333;
  box-sizing: border-box;
}

.topic-list .btn .liked {
  border: 2rpx solid #f5f5f5;
  color: #ccc;
  background: #f5f5f5;
}

.topic-list .logo {
  display: flex;
  margin-top: 17rpx;
  height: 36rpx;
}

.topic-list .logo image {
  overflow: hidden;
  width: 36rpx;
  height: 36rpx;
  border-radius: 100%;
}

.topic-list .logo text {
  margin-left: 8rpx;
  height: 36rpx;
  font-size: 26rpx;
  font-weight: 400;
  color: #222;
  line-height: 36rpx;
}

.topic-list .author {
  overflow: hidden;
  margin-top: 24rpx;
  height: 30rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, "PingFang SC";
  font-weight: 400;
  text-overflow: ellipsis;
  white-space: nowrap;
  color: #999;
  line-height: 30rpx;
}

.topic-list .tags {
  display: flex;
  overflow: hidden;
  padding-top: 8rpx;
  flex-wrap: wrap;
}

.topic-list .tags text {
  margin-top: 10rpx;
  margin-right: 16rpx;
  padding: 0 16rpx;
  height: 40rpx;
  font-size: 22rpx;
  font-family: PingFangSC-Regular, "PingFang SC";
  font-weight: 400;
  border-radius: 22rpx;
  white-space: nowrap;
  color: #666;
  background: #f2f2f2;
  line-height: 40rpx;
}

.topic-list .des {
  display: block;
  overflow: hidden;
  width: 100%;
  font-size: 24rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  color: #999;
}

.topic-list .bottom {
  position: absolute;
  bottom: 8rpx;
  left: 24rpx;
  display: flex;
  width: 100%;
  height: 36rpx;
  flex-direction: row;
  flex-wrap: nowrap;
}

.topic-list .bottom .praise,
.topic-list .bottom .comment {
  display: flex;
  align-items: center;
  margin-right: 32rpx;
  height: 36rpx;
}

.topic-list .bottom .praise image,
.topic-list .bottom .comment image {
  margin-right: 4rpx;
  width: 36rpx;
  height: 36rpx;
  vertical-align: top;
}

.topic-list .bottom .praise text,
.topic-list .bottom .comment text {
  font-size: 28rpx;
  font-family: "PingFang SC";
  font-weight: 400;
  color: #999;
  font-style: normal;
}

/* follow */
.follow {
  position: absolute;
  z-index: 2;
  width: 96rpx;
  height: 44rpx;
  font-size: 22rpx;
  border-radius: 44rpx;
  text-align: center;
  color: #442509;
  background: #fde23d;
  line-height: 44rpx;
}

.followed {
  color: #333;
  background: #fff;
  opacity: 0.8;
}

/* loading */
.loading {
  display: inline-block;
  width: 20px;
  height: 20px;
  background: transparent url("data:image/svg+xml;base64,") no-repeat;
  background-size: 100%;
  vertical-align: middle;
  animation: weuiLoading 1s steps(12, end) infinite;
}

/* loadmore */
.loadmore-wrap {
  overflow: hidden;
}

.loadmore {
  position: relative;
  margin: 21px auto 0;
  width: 65%;
  font-size: 24rpx;
  text-align: center;
  line-height: 20px;
  vertical-align: middle;
}

.loadmore .loading {
  margin: -22px 4px 0 0;
}

.loadmore-tips {
  position: relative;
  top: -11px;
  z-index: 1;
  display: inline-block;
  padding: 0 6px;
  height: 20px;
  line-height: 20px;
  vertical-align: middle;
}

.loadmore-nodata,
.loadmore-nomore {
  color: #81858a;
}

.loadmore-nodata::after,
.loadmore-nomore::after {
  content: "";
  position: absolute;
  top: 0;
  left: 0;
  width: 200%;
  height: 200%;
  transform: scale(0.5);
  transform-origin: 0 0;
  pointer-events: none;
  box-sizing: border-box;
  border: 0 solid #e5e5e5;
  border-top-width: 1px;
}

.loadmore-nodata {
  margin-top: 120px;
}

.history-item view {
  display: flex;
  align-items: center;
}

.searchnew-history {
  padding-top: 24rpx;
}

.searchnew-history-content {
  display: flex;
  justify-content: flex-start;
  padding: 12rpx 24rpx 8rpx;
  flex-wrap: wrap;
}

.searchnew-history-item {
  margin: 0rpx 16rpx 24rpx 0rpx;
  padding: 0rpx 24rpx;
  height: 60rpx;
  font-size: 26rpx;
  border-radius: 30rpx;
  text-align: center;
  color: #333;
  background: #f7f7f8;
  line-height: 60rpx;
  box-sizing: border-box;
}

.searchnew-history-item .tag {
  overflow: hidden;
  max-width: 260rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.searchnew-history-item .ico-more {
  margin-top: 15rpx;
  width: 32rpx;
  height: 32rpx;
}

.searchnew-hot {
  padding-top: 24rpx;
}

.searchnew-hot-content {
  display: flex;
  justify-content: space-between;
  padding: 12rpx 24rpx 0;
  flex-wrap: wrap;
}

.searchnew-hot-item {
  display: flex;
  margin-bottom: 32rpx;
  width: 330rpx;
}

.searchnew-hot-img {
  overflow: hidden;
  width: 100rpx;
  height: 100rpx;
  border-radius: 12rpx;
}

.searchnew-hot-info {
  padding-left: 16rpx;
  min-width: 0;
  flex: 1;
}

.searchnew-hot-title {
  display: block;
  overflow: hidden;
  height: 50rpx;
  font-size: 30rpx;
  font-weight: 500;
  text-overflow: ellipsis;
  white-space: nowrap;
  color: #333;
  line-height: 50rpx;
}

.searchnew-hot-subtitle {
  overflow: hidden;
  margin-top: 8rpx;
  height: 38rpx;
  font-size: 26rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  color: #999;
  line-height: 38rpx;
}

.search-rec-tips {
  padding-top: 30rpx;
  background-color: #fff;
}

.search-rec-tips .rec-tips-box {
  display: flex;
  justify-content: center;
  align-items: center;
  line-height: 34rpx;
}

.search-rec-tips .rec-tips-box text {
  font-size: 28rpx;
  font-family: "PingFang SC";
  font-weight: 400;
  color: #666;
  font-style: normal;
}

.search-rec-tips .rec-tips-box .active {
  overflow: hidden;
  max-width: 300rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  color: #ff751a;
}

.search-rec-tips .rec-tips-title {
  margin-top: 52rpx;
  padding: 0 24rpx;
  font-size: 36rpx;
  font-family: "PingFang SC";
  font-weight: 500;
  color: #222;
  font-style: normal;
  line-height: 60rpx;
}
