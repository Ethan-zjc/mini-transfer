page {
  min-height: 100%;
}

.container {
  position: relative;
  min-height: 100%;
}

button {
  position: relative;
  background: none;
}

button:not([size="mini"]) {
  padding: 0;
  line-height: inherit;
  color: inherit;
  font-size: inherit;
  font-weight: inherit;
}

button[type="primary"] {
  background: #FED23D !important;
  color: #442509 !important;
}

button[type="primary"]::after {
  position: absolute;
  left: 0;
  top: 0;
  width: 200%;
  height: 200%;
  border: 1px solid #FED23D;
}

button[type="primary"][disabled] {
  opacity: 0.6;
}

button[disabled] {
  background: #f9f6ff !important;
  color: #cfc6e9 !important;
}

image {
  display: block;
}

.kkmh-empty {
  width: 100%;
  font-size: 24rpx;
  text-align: center;
  padding-top: 120px;
  color: #81858a;
}

.kkmh-empty_text {
  display: flex;
  position: relative;
  align-items: center;
  justify-content: center;
}

.kkmh-empty_text:before, .kkmh-empty_text:after {
  content: "";
  display: block;
  width: 180rpx;
  height: 1px;
  transform: scaleY(0.5);
  transform-origin: 0 0;
  pointer-events: none;
  box-sizing: border-box;
  border: 0 solid #e5e5e5;
  border-top-width: 1px;
  margin: 0 12rpx;
}

.kkmh-empty_img {
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}

.kkmh-empty_img .img {
  width: 240rpx;
  height: 240rpx;
}

.kkmh-empty_img .text {
  width: 596rpx;
  height: 66rpx;
  margin-top: 20rpx;
}

.kk-bottom {
  display: flex;
  justify-content: center;
  align-items: center;
  overflow: hidden;
  position: relative;
}

.kk-bottom .weui-loadmore {
  display: flex;
  flex-direction: row-reverse;
}

.kk-bottom .weui-loadmore__tips {
  display: inline-block;
}

.weui-loading {
  display: inline-block;
  margin: 0 10rpx;
  width: 40rpx;
  height: 40rpx;
  vertical-align: middle;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}

