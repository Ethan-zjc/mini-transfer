@keyframes weuiLoading {
  0% {
    transform: rotate3d(0, 0, 1, 0deg);
  }
  100% {
    transform: rotate3d(0, 0, 1, 360deg);
  }
}

.toast .weui-loading {
  margin: 24rpx auto;
  width: 60rpx;
  height: 60rpx;
}

.toast .weui-loadmore__tips {
  display: block;
  margin-bottom: 14rpx;
}

.get-phone-number-button {
  width: 184px;
}

.get-phone-number-button:after {
  border: none;
}

.base-holder {
  z-index: 0;
  background: transparent url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}
