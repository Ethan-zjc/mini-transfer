.wrap {
  position: relative;
  box-sizing: border-box;
  width: 100%;
  height: 100vh;
  background: #fafafa;
  padding-top: 20rpx;
}

.wrap .safe {
  width: 100%;
  position: fixed;
  bottom: 20rpx;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
  display: flex;
  align-items: center;
  justify-content: center;
}

.wrap .btn {
  width: 680rpx;
  height: 88rpx;
  text-align: center;
  line-height: 88rpx;
  color: #000;
  font-size: 28rpx;
  background: #FCE23D;
  margin: auto;
  border-radius: 90rpx;
  font-weight: 500;
}

.list .title {
  font-size: 28rpx;
  padding-left: 32rpx;
  margin-bottom: 10rpx;
  color: #999;
}

.list .contain {
  padding-left: 32rpx;
  height: 88rpx;
  background: #fff;
  display: flex;
  align-items: center;
}

.list .prefix {
  font-size: 32rpx;
  width: 200rpx;
}

.list .suffix {
  flex: 1;
  padding-left: 60rpx;
  font-size: 28rpx;
}
