.video-list .list {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  padding: 32rpx 24rpx;
}

.video-list .stance {
  height: 132rpx;
}

.video-list .bottom-wrap {
  position: fixed;
  bottom: 0;
  padding: 16rpx 24rpx 42rpx;
  height: 158rpx;
  width: 100%;
  background: #ffffff;
  box-sizing: border-box;
}

.video-list .bottom-wrap .vip {
  position: relative;
  display: flex;
  justify-content: space-between;
  width: 100%;
  height: 100%;
  padding: 12rpx 12rpx 12rpx 48rpx;
  background: linear-gradient(90deg, rgba(255, 255, 255, 0.3) 0%, rgba(255, 255, 255, 0.4) 0.01%, rgba(255, 255, 255, 0) 100%), #775BFA;
  border-radius: 52rpx;
  box-sizing: border-box;
}

.video-list .bottom-wrap .vip .text {
  padding-top: 36rpx;
  font-size: 26rpx;
  font-weight: 500;
  color: #ffffff;
}

.video-list .bottom-wrap .vip .btn {
  width: 212rpx;
  height: 76rpx;
  line-height: 76rpx;
  background: #FFE120;
  border-radius: 52rpx;
  color: #222222;
  font-size: 30rpx;
  text-align: center;
  font-weight: 500;
}

.video-list .bottom-wrap .vip image {
  position: absolute;
  top: -34rpx;
  left: 48rpx;
  width: 116rpx;
  height: 80rpx;
}
