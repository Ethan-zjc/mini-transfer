.success-popup-mask {
  position: fixed;
  top: 0;
  width: 100%;
  z-index: 999999;
  display: flex;
  overflow: hidden;
  align-items: center;
  justify-content: center;
}

.success-popup-mask.success-show-mask {
  bottom: 0;
  height: 100vh;
  animation: fade-in ease-in-out 0.4s 1 alternate forwards;
}

.success-popup-mask.success-hide-mask {
  height: 0;
}

.success-popup-mask .preloading {
  display: none !important;
}

.success-popup-mask .success-warp-dialog {
  z-index: 1;
  width: 576rpx;
  height: auto;
  position: relative;
  opacity: 0;
  background: #fff;
  border-radius: 24rpx;
  box-sizing: border-box;
}

.success-popup-mask .success-warp-dialog.success-show-contain {
  animation: popup-animate 0.4s linear forwards;
}

.success-popup-mask .success-warp-dialog .rule-popup {
  height: auto;
}

.success-popup-mask .success-warp-dialog .rule-popup .rule-popup-titel {
  padding: 48rpx 0 26rpx 0;
  font-size: 36rpx;
  font-family: PingFangSC-Semibold,PingFang SC;
  font-weight: 600;
  color: #333333;
  line-height: 46rpx;
  text-align: center;
}

.success-popup-mask .success-warp-dialog .rule-popup .rule-popup-text {
  padding: 0 56rpx;
}

.success-popup-mask .success-warp-dialog .rule-popup .rule-popup-text .item {
  font-size: 30rpx;
  font-family: PingFangSC-Regular,PingFang SC;
  font-weight: 400;
  color: #333333;
  line-height: 50rpx;
}

.success-popup-mask .success-warp-dialog .rule-popup .rule-popup-text .item .rule-rich-text {
  font-size: 30rpx;
}

.success-popup-mask .success-warp-dialog .rule-popup .rule-popup-text .item .rule-rich-text .b {
  padding-right: 8rpx;
  font-weight: normal;
}

.success-popup-mask .success-warp-dialog .rule-popup .rule-popup-btn {
  margin: 40rpx auto 48rpx;
  width: 380rpx;
  height: 80rpx;
  background: #ffe23d;
  border-radius: 40rpx;
  text-align: center;
  line-height: 80rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Medium,PingFang SC;
  font-weight: 500;
  color: #442509;
}

.success-popup-mask .reward-popup {
  box-sizing: border-box;
  position: relative;
  width: 100%;
  height: auto;
  min-height: 200rpx;
  border-radius: 24rpx;
}

.success-popup-mask .reward-popup .success-icon-wrap {
  position: absolute;
  width: 168rpx;
  height: 168rpx;
  left: 0;
  right: 0;
  top: -84rpx;
  margin: 0 auto;
  z-index: 10;
}

.success-popup-mask .reward-popup .success-icon-wrap .icon {
  width: 100%;
  height: 100%;
}

.success-popup-mask .reward-popup .success-title-wrap {
  position: relative;
  padding: 56rpx 88rpx 0;
  height: 80rpx;
}

.success-popup-mask .reward-popup .success-title-wrap .title-bg {
  width: 100%;
  height: 100%;
}

.success-popup-mask .reward-popup .success-title-wrap .title {
  position: absolute;
  width: 100%;
  height: 80rpx;
  z-index: 1;
  text-align: center;
  left: 0;
  top: 56rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.success-popup-mask .reward-popup .success-title-wrap .title text {
  padding-top: 28rpx;
  font-size: 40rpx;
  font-family: PingFangSC-Semibold,PingFang SC;
  font-weight: 600;
  color: #ff751a;
  line-height: 48rpx;
}

.success-popup-mask .reward-popup .success-subtitle-wrap {
  padding-top: 8rpx;
  height: 30rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular,PingFang SC;
  font-weight: 400;
  color: #999999;
  line-height: 30rpx;
  text-align: center;
}

.success-popup-mask .reward-popup .success-subtitle-wrap .rich-text .b {
  padding: 0 6rpx;
  color: #FFBA15;
  font-weight: 500;
}

.success-popup-mask .reward-popup .success-content-wrap {
  padding: 32rpx 0 48rpx 0;
  height: 120rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.success-popup-mask .reward-popup .success-content-wrap .reward-iocn {
  width: 120rpx;
  height: 120rpx;
}

.success-popup-mask .reward-popup .success-content-wrap .reward-iocn .iocn {
  width: 120rpx;
  height: 120rpx;
}

.success-popup-mask .reward-popup .success-content-wrap .reward-content {
  padding-left: 16rpx;
  font-size: 36rpx;
  font-family: PingFangSC-Medium,PingFang SC;
  font-weight: 500;
  color: #333333;
}

.success-popup-mask .reward-popup .success-btn-wrap {
  margin: 0 auto;
  margin-bottom: 48rpx;
  width: 380rpx;
  height: 80rpx;
  background: #ffe23d;
  border-radius: 40rpx;
  text-align: center;
  line-height: 80rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Medium,PingFang SC;
  font-weight: 500;
  color: #442509;
}

@keyframes fade-in {
  0% {
    background-color: rgba(0, 0, 0, 0.1);
  }
  100% {
    background-color: rgba(0, 0, 0, 0.7);
  }
}

@keyframes popup-animate {
  0% {
    opacity: 0;
  }
  100% {
    opacity: 1;
  }
}
