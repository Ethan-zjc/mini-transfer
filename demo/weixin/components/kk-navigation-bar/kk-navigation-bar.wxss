.kk-nav-bar {
  position: relative;
  z-index: 9999;
  width: 100%;
  text-align: center;
}

.kk-nav-bar .kk-nav-bar__inner {
  width: 100%;
  height: 100%;
  position: relative;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
}

.kk-nav-bar .kk-nav-bar__inner .kk-nav-bar__left {
  z-index: 1;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.kk-nav-bar .kk-nav-bar__inner .kk-nav-bar__left .arrow-back {
  width: 48rpx;
  margin-left: 24rpx;
}

.kk-nav-bar .kk-nav-bar__inner .kk-nav-bar__left .arrow-back image {
  width: 48rpx;
  height: 48rpx;
}

.kk-nav-bar .kk-nav-bar__inner .kk-nav-bar__left .arrow-home {
  width: 48rpx;
  margin-left: 32rpx;
}

.kk-nav-bar .kk-nav-bar__inner .kk-nav-bar__left .arrow-home image {
  width: 48rpx;
  height: 48rpx;
}

.kk-nav-bar .kk-nav-bar__inner .kk-nav-bar__left .whrite-back {
  width: 48rpx;
  margin-left: 32rpx;
}

.kk-nav-bar .kk-nav-bar__inner .kk-nav-bar__left .whrite-back image {
  width: 42rpx;
  height: 48rpx;
}

.kk-nav-bar .kk-nav-bar__inner .kk-nav-bar__center {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 16px;
  color: #000;
  font-weight: 400;
}
