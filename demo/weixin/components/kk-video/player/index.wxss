page {
  height: 100%;
}

.container {
  position: relative;
  min-height: 100%;
}

button {
  position: relative;
  background: none;
}

button:not([size="mini"]) {
  padding: 0;
  line-height: inherit;
  color: inherit;
  font-size: inherit;
  font-weight: inherit;
}

button[type="primary"] {
  background: #FED23D !important;
  color: #442509 !important;
}

button[type="primary"]::after {
  position: absolute;
  left: 0;
  top: 0;
  width: 200%;
  height: 200%;
  border: 1px solid #FED23D;
}

button[type="primary"][disabled] {
  opacity: 0.6;
}

button[disabled] {
  background: #f9f6ff !important;
  color: #cfc6e9 !important;
}

image {
  display: block;
}

.ui-loading {
  display: inline-block;
  margin: 0 10rpx;
  width: 40rpx;
  height: 40rpx;
  display: inline-block;
  vertical-align: middle;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}

@keyframes weuiLoading {
  0% {
    transform: rotate3d(0, 0, 1, 0deg);
  }
  100% {
    transform: rotate3d(0, 0, 1, 360deg);
  }
}

.toast .weui-loading {
  margin: 24rpx auto;
  width: 60rpx;
  height: 60rpx;
}

.toast .weui-loadmore__tips {
  display: block;
  margin-bottom: 14rpx;
}

.get-phone-number-button {
  width: 184px;
}

.get-phone-number-button:after {
  border: none;
}

.base-holder {
  z-index: 0;
  background: transparent url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}

.base-player {
  background: transparent url(data:image/gif;base64,) no-repeat;
  background-size: 100%;
}

.video-wrap {
  position: relative;
  width: 100%;
  height: 100vh;
}

.video-box {
  width: 100vw;
  height: 100vh;
  background-color: #fff;
  position: relative;
  z-index: 1;
}

.video-play {
  position: absolute;
  left: 0;
  bottom: 0;
  z-index: 20;
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.video-play image {
  height: 96rpx;
  width: 96rpx;
}

.video-pic {
  position: absolute;
  left: 0;
  top: 0;
  z-index: 10;
  width: 100%;
  height: 100%;
}

.video-pic .img {
  width: 100%;
  height: 100%;
}

.video-loading {
  position: absolute;
  z-index: 11;
  width: 216rpx;
  height: 216rpx;
  background: #000;
  position: absolute;
  left: 50%;
  top: 50%;
  background: rgba(0, 0, 0, 0.7);
  border-radius: 16rpx;
  transform: translate(-50%, -50%);
  display: flex;
  align-items: center;
  justify-content: center;
}

.video-loading .dot {
  width: 60rpx;
  height: 60rpx;
}

.video-bottom {
  position: absolute;
  left: 0;
  bottom: 0;
  z-index: 30;
  width: 100%;
}

.video-full {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  z-index: 20;
  height: 310rpx;
  background: linear-gradient(180deg, rgba(0, 0, 0, 0.0001) 0%, rgba(0, 0, 0, 0.501925) 100%);
  transform: translateZ(0);
}

.video-full-iphone {
  height: 376rpx;
}

.blank {
  position: relative;
  z-index: 3;
  height: 66rpx;
  background: none;
  width: 100%;
}
