.kk-novice-login-benefits {
  box-sizing: border-box;
  padding: 24rpx 16rpx;
  width: 100%;
  height: 428rpx;
}

.kk-novice-login-benefits .receive-card {
  box-sizing: border-box;
  width: 718rpx;
  min-height: 379rpx;
  background: #FFFFFF;
  box-shadow: 0px 8rpx 40rpx 0px rgba(0, 0, 0, 0.02), 0px 4rpx 24rpx 0rpx rgba(0, 0, 0, 0.02), 0px 0px 16rpx 0rpx rgba(0, 0, 0, 0.01);
  border-radius: 24rpx;
  border: 2rpx solid rgba(239, 243, 244, 0.5);
}

.kk-novice-login-benefits .header {
  box-sizing: border-box;
  padding: 32rpx;
  width: 100%;
  display: flex;
}

.kk-novice-login-benefits .header .placeholder {
  flex: 1;
}

.kk-novice-login-benefits .header .left .images {
  width: 40rpx;
  height: 40rpx;
}

.kk-novice-login-benefits .header .title {
  display: flex;
  align-items: center;
}

.kk-novice-login-benefits .header .title-text {
  height: 40rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: #333333;
  line-height: 40rpx;
}

.kk-novice-login-benefits .header .left-time {
  padding-top: 8rpx;
  height: 30rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #F56C6C;
  line-height: 30rpx;
}

.kk-novice-login-benefits .header .right {
  position: relative;
  margin-top: 8rpx;
  display: flex;
  align-items: center;
  width: 128rpx;
  height: 56rpx;
  background: #FDE23D;
  border-radius: 28rpx;
}

.kk-novice-login-benefits .header .right .btn {
  position: absolute;
  left: 0;
  top: 0;
  z-index: 1;
  width: 100%;
  height: 100%;
  background: none;
}

.kk-novice-login-benefits .header .right .text {
  padding-left: 32rpx;
  font-size: 26rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #282028;
  line-height: 26rpx;
}

.kk-novice-login-benefits .header .right .images {
  width: 26rpx;
  height: 26rpx;
}

.kk-novice-login-benefits .bodys {
  box-sizing: border-box;
  padding: 0 56rpx 20rpx;
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  justify-content: space-between;
}

.kk-novice-login-benefits .bodys .item {
  box-sizing: border-box;
  position: relative;
  padding-top: 20rpx;
  width: 158rpx;
  height: 180rpx;
  background: #F7F7F8;
  border-radius: 16rpx;
}

.kk-novice-login-benefits .bodys .title {
  font-size: 22rpx;
  font-family: SFProText-Medium, SFProText;
  font-weight: 500;
  color: rgba(0, 0, 0, 0.7);
  height: 28rpx;
  line-height: 28rpx;
  text-align: center;
}

.kk-novice-login-benefits .bodys .item-icon {
  margin: 10rpx auto 14rpx;
  width: 64rpx;
  height: 64rpx;
}

.kk-novice-login-benefits .bodys .day-text {
  font-size: 20rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(0, 0, 0, 0.5);
  text-align: center;
}

.kk-novice-login-benefits .bodys .down-arrow {
  position: absolute;
  bottom: -7rpx;
  left: 0;
  right: 0;
  margin: 0 auto;
  width: 0;
  height: 0;
  border-left: 10rpx solid transparent;
  border-right: 10rpx solid transparent;
  border-top: 8rpx solid #F7F7F8;
}

.kk-novice-login-benefits .bodys .dot {
  position: absolute;
  bottom: -26rpx;
  left: 0;
  right: 0;
  margin: 0 auto;
  box-sizing: border-box;
  width: 12rpx;
  height: 12rpx;
  background: #F7F9FA;
  border: 2rpx solid #EEEEEE;
  z-index: 1;
  border-radius: 50%;
  overflow: hidden;
}

.kk-novice-login-benefits .bodys .item-active {
  background: #FDE23D;
}

.kk-novice-login-benefits .bodys .item-active .down-arrow {
  border-top: 8rpx solid #FDE23D;
}

.kk-novice-login-benefits .bodys .item-active .dot {
  background: #FDE23D;
  border: 2rpx solid #FFBA15;
}

.kk-novice-login-benefits .footer {
  position: relative;
  height: 2rpx;
  min-height: 1px;
  width: 100%;
  background: #EEEEEE;
}

.kk-novice-login-benefits .footer .content {
  width: 0;
  height: 2rpx;
  min-height: 1px;
  background: #FFBA15;
}

.kk-novice-login-benefits .footer .content.content-1 {
  width: 138rpx;
}

.kk-novice-login-benefits .footer .content.content-2 {
  width: 356rpx;
}

.kk-novice-login-benefits .footer .content.content-3 {
  width: 100%;
}

.kk-novice-rule-popup, .kk-clockin-rulepopup {
  position: fixed;
  left: 0;
  top: 0;
  z-index: 9999;
  width: 100%;
  height: 100vh;
  background: rgba(0, 0, 0, 0.7);
  display: flex;
  justify-content: center;
  align-items: center;
}

.kk-novice-rule-popup .content, .kk-clockin-rulepopup .content {
  width: 576rpx;
  background: #FFFFFF;
  border-radius: 24rpx;
}

.kk-novice-rule-popup .title, .kk-clockin-rulepopup .title {
  padding-top: 48rpx;
  padding-bottom: 26rpx;
  font-size: 36rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #333333;
  line-height: 48rpx;
  text-align: center;
}

.kk-novice-rule-popup .text, .kk-clockin-rulepopup .text {
  padding: 0 56rpx 26rpx;
  font-size: 30rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #999999;
  line-height: 40rpx;
  text-align: justify;
}

.kk-novice-rule-popup .text-bottom, .kk-clockin-rulepopup .text-bottom {
  padding-bottom: 42rpx;
  border-bottom: 2rpx solid #EEEEEE;
}

.kk-novice-rule-popup .close-btn, .kk-clockin-rulepopup .close-btn {
  font-size: 36rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #FF751A;
  line-height: 104rpx;
  text-align: center;
}

.kk-clockin-rulepopup {
  position: fixed;
  left: 0;
  top: 0;
  z-index: 9999;
  width: 100%;
  height: 100vh;
  background: rgba(0, 0, 0, 0.7);
  display: flex;
  justify-content: center;
  align-items: center;
}

