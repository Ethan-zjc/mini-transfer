.kk-clockin-cards {
  background: #fff;
  margin-top: 12rpx;
  padding: 28rpx 24rpx;
}

.kk-clockin-cards .clockin-card {
  box-sizing: border-box;
  width: 100%;
  height: auto;
  background: #FFFFFF;
  transition: all 1s linear;
}

.kk-clockin-cards .header {
  box-sizing: border-box;
  display: flex;
  align-items: center;
  width: 100%;
}

.kk-clockin-cards .header .title-warp {
  display: flex;
  align-items: center;
}

.kk-clockin-cards .header .title {
  height: 32rpx;
  line-height: 32rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #333333;
}

.kk-clockin-cards .header .arrow-btn {
  padding: 2rpx 0 2rpx 26rpx;
}

.kk-clockin-cards .header .images-box, .kk-clockin-cards .header .images-box .images, .kk-clockin-cards .header .arrow-btn, .kk-clockin-cards .header .arrow-btn .images {
  width: 38rpx;
  height: 38rpx;
}

.kk-clockin-cards .header .arrow-btn-down .images {
  transform: rotate(180deg);
}

.kk-clockin-cards .header .tomorrow-text {
  padding-top: 12rpx;
  height: 24rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #999999;
  line-height: 24rpx;
}

.kk-clockin-cards .header .station-symbol {
  flex: 1;
  height: 2rpx;
}

.kk-clockin-cards .header .section-warp {
  display: flex;
  align-items: center;
}

.kk-clockin-cards .header .section-warp .section-title {
  padding-right: 12rpx;
  height: 22rpx;
  font-size: 22rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #999999;
  line-height: 22rpx;
}

.kk-clockin-cards .header .section-warp .my-switch {
  box-sizing: border-box;
  display: flex;
  align-items: center;
  width: 72rpx;
  height: 42rpx;
  position: relative;
  border-radius: 20rpx;
  border: 1rpx solid #EEEEEE;
  overflow: hidden;
  background: #FFD54C;
}

.kk-clockin-cards .header .section-warp .my-switch::before {
  display: block;
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  border-radius: 9999rpx;
  background: #fff;
  transition: all 0.35s cubic-bezier(0.45, 1, 0.4, 1);
}

.kk-clockin-cards .header .section-warp .my-switch-checked::before {
  transform: scale(0);
}

.kk-clockin-cards .header .section-warp .my-switch .btn {
  position: absolute;
  top: 6rpx;
  bottom: 6rpx;
  left: 6rpx;
  margin: auto 0;
  width: 28rpx;
  height: 28rpx;
  border-radius: 50%;
  background: #FFD54C;
  transition: all 0.35s cubic-bezier(0.45, 1, 0.4, 1);
}

.kk-clockin-cards .header .section-warp .my-switch-checked .btn {
  background: #FFFFFF;
  left: 64rpx;
  transform: translateX(-100%);
}

.kk-clockin-cards .header .see-btn {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 128rpx;
  height: 57rpx;
  background: #FDE23D;
  border-radius: 28rpx;
  overflow: hidden;
}

.kk-clockin-cards .header .see-btn .btn-img {
  width: 100%;
  height: 100%;
}

.kk-clockin-cards .list-box {
  padding-top: 30rpx;
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  justify-content: space-between;
  height: 124rpx;
}

.kk-clockin-cards .list-box .list-item {
  position: relative;
  width: 90rpx;
  height: 110rpx;
  background: #F8F8F8;
  border-radius: 12rpx;
}

.kk-clockin-cards .list-box .list-item .kkb-warp {
  position: relative;
  margin: 0 auto;
  padding: 12rpx 0 16rpx 0;
  width: 48rpx;
  height: 48rpx;
}

.kk-clockin-cards .list-box .list-item .kkb-warp .images {
  width: 48rpx;
  height: 48rpx;
}

.kk-clockin-cards .list-box .list-item .day-text {
  font-size: 22rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #999;
  line-height: 30rpx;
  text-align: center;
}

.kk-clockin-cards .list-box .item-bg-receive .kkb-warp, .kk-clockin-cards .list-box .item-bg-sign .kkb-warp {
  position: relative;
  margin: 0 auto;
  padding: 6rpx 0 4rpx 0;
  width: 69rpx;
  height: 67rpx;
}

.kk-clockin-cards .list-box .item-bg-receive .kkb-warp .images, .kk-clockin-cards .list-box .item-bg-sign .kkb-warp .images {
  width: 69rpx;
  height: 67rpx;
}

.kk-clockin-cards .list-box .item-bg-receive .day-text, .kk-clockin-cards .list-box .item-bg-sign .day-text {
  color: #FFFFFF;
}

.kk-clockin-cards .list-box .item-bg-receive {
  background: #FF8C46;
}

.kk-clockin-cards .list-box .item-bg-sign {
  background: #FEB138;
}

