.comic-recommend {
  padding-bottom: 24rpx;
}

.comic-recommend-border {
  border-top: 1px solid #e8e8e8;
}

.comic-recommend-home {
  width: 702rpx;
  height: 80rpx;
  background: #FCE140;
  border-radius: 100rpx;
  margin: 24rpx auto 0;
  display: flex;
  align-items: center;
  justify-content: center;
}

.comic-recommend-home image {
  width: 36rpx;
  height: 36rpx;
}

.comic-recommend-home text {
  color: #442509;
  font-size: 28rpx;
  font-weight: 400;
  margin-left: 12rpx;
}

.comic-recommend-top {
  height: 104rpx;
  display: flex;
  align-items: center;
}

.comic-recommend-top .recommend-top-title {
  font-size: 32rpx;
  font-weight: 500;
  color: #333;
  padding-left: 24rpx;
  padding-right: 24rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  flex: 1;
}

.comic-recommend-top .recommend-top-more {
  font-size: 24rpx;
  color: #999;
  padding-right: 32rpx;
  display: flex;
  align-items: center;
}

.comic-recommend-top .recommend-top-more image {
  width: 24rpx;
  height: 24rpx;
}

.comic-recommend-content {
  padding-bottom: 24rpx;
}

.comic-recommend-content .more {
  background: rgba(0, 0, 0, 0.05);
  border-radius: 8rpx;
  height: 290rpx;
  width: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  flex-shrink: 0;
}

.comic-recommend-content .more-icon {
  margin-bottom: 8rpx;
}

.comic-recommend-content .more-icon image {
  width: 26rpx;
  height: 26rpx;
}

.comic-recommend-content .more-text {
  font-size: 26rpx;
  line-height: 37rpx;
  color: #999;
  display: flex;
  flex-direction: column;
}

.comic-recommend-rank {
  margin-top: -12rpx;
  height: 390rpx;
  overflow: hidden;
}

.comic-recommend-rank .recommend-rank-list {
  display: flex;
  height: auto;
  padding-bottom: 20rpx;
  padding-top: 12rpx;
  padding-left: 24rpx;
}

.comic-recommend-rank .recommend-rank-box {
  padding-right: 16rpx;
  width: 218rpx;
}

.comic-recommend-rank .recommend-rank-box .info {
  width: 218rpx;
  height: 290rpx;
  position: relative;
  border-radius: 12rpx;
}

.comic-recommend-rank .recommend-rank-box .info .info-cover {
  overflow: hidden;
  border-radius: 12rpx;
}

.comic-recommend-rank .recommend-rank-box .info .info-tips {
  background: linear-gradient(180deg, transparent 0%, rgba(0, 0, 0, 0.5) 100%);
  border-radius: 0px 0px 4rpx 4rpx;
  height: 60rpx;
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  text-align: right;
}

.comic-recommend-rank .recommend-rank-box .info .info-tips text {
  font-weight: 500;
  font-size: 20rpx;
  color: #fff;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  padding: 24rpx 12rpx 0 0;
  display: block;
}

.comic-recommend-rank .recommend-rank-box .info .info-tag {
  position: absolute;
  width: 56rpx;
  height: 65rpx;
  left: 10rpx;
  top: -12rpx;
  text-align: center;
  display: flex;
  flex-direction: column;
}

.comic-recommend-rank .recommend-rank-box .info .info-tag image {
  position: relative;
  width: 56rpx;
  height: 65rpx;
}

.comic-recommend-rank .recommend-rank-box .info .info-tag-top {
  font-size: 16rpx;
  color: #fff;
  font-weight: bold;
  font-family: DINAlternate-Bold, DINAlternate;
  position: absolute;
  left: 0;
  top: 8rpx;
  width: 100%;
  text-align: center;
}

.comic-recommend-rank .recommend-rank-box .info .info-tag-num {
  font-size: 24rpx;
  color: #fff;
  font-weight: bold;
  font-family: DINAlternate-Bold, DINAlternate;
  position: absolute;
  left: 0;
  top: 24rpx;
  width: 100%;
  text-align: center;
}

.comic-recommend-rank .recommend-rank-box .title {
  font-size: 28rpx;
  color: #333;
  font-weight: 500;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin-top: 8rpx;
  height: 50rpx;
  line-height: 50rpx;
}

.comic-recommend-rank .recommend-rank-box .subtitle {
  font-size: 24rpx;
  height: 30rpx;
  line-height: 30rpx;
  color: #999;
  font-weight: 400;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

