.dialog-error {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 99;
  background: rgba(0, 0, 0, 0.9);
}

.dialog-error .error-wrap {
  width: 750rpx;
  position: absolute;
  left: 0;
  top: 45%;
  transform: translateY(-50%);
}

.dialog-error .error-title {
  width: 100%;
  line-height: 100rpx;
  text-align: center;
  font-size: 50rpx;
  font-family: PingFang SC;
  font-weight: 600;
  color: #FFFFFF;
}

.dialog-error .error-info {
  width: 426rpx;
  height: 426rpx;
  margin: 0 auto;
  position: relative;
}

.dialog-error .error-info image {
  width: 100%;
  height: 100%;
}

.dialog-error .error-close {
  width: 358rpx;
  height: 84rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 34rpx auto 0;
  overflow: hidden;
  position: relative;
}

.dialog-error .error-close image {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 1;
}

.dialog-error .error-close text {
  position: relative;
  z-index: 2;
  line-height: 84rpx;
  font-size: 36rpx;
  font-family: PingFang SC;
  font-weight: 600;
  color: #fff;
  width: 100%;
  text-align: center;
}
