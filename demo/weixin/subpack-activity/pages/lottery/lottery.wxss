.lottery-wrap {
  min-height: 100vh;
  padding-top: 40rpx;
  padding-bottom: 90rpx;
  background: #F7F7F8;
}

.lottery-navigator {
  position: fixed;
  top: 0rpx;
  left: 16rpx;
  padding: 0 10rpx;
  width: 68rpx;
  height: 68rpx;
  z-index: 9990;
}

.lottery-navigator .icon-img {
  width: 68rpx;
  height: 68rpx;
}

.lottery-banner {
  overflow: hidden;
  padding: 0rpx 24rpx 0rpx 24rpx;
  margin-bottom: 24rpx;
}

.lottery-banner image {
  display: block;
  width: 100%;
}

.lottery-main {
  width: 702rpx;
  margin: 0 auto 24rpx;
  padding: 0 24rpx;
  box-sizing: border-box;
  border-radius: 20rpx;
  background-color: #fff;
}

.lottery-main .main-title {
  height: 98rpx;
  line-height: 98rpx;
  font-size: 36rpx;
  font-family: Source Han Sans CN;
  font-weight: 500;
  color: #3A4356;
  display: flex;
  align-items: center;
}

.lottery-main .main-title .title {
  flex: 1;
}

.lottery-main .main-title .rule {
  font-size: 26rpx;
  color: #666;
}

.lottery-main .main-info {
  position: relative;
}

.lottery-main .main-ant {
  height: 900rpx;
}

.lottery-main .main-record {
  position: absolute;
  right: 0;
  top: 150rpx;
  width: 60rpx;
  height: 196rpx;
  z-index: 5;
}

.lottery-main .main-record .button {
  width: 60rpx;
  height: 196rpx;
}

.lottery-main .main-record image {
  width: 100%;
  height: 100%;
}

.lottery-main .lottery-login {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  overflow: hidden;
  opacity: .2;
}

.lottery-task {
  width: 702rpx;
  margin: 0 auto;
  padding: 0 24rpx 60rpx;
  box-sizing: border-box;
  border-radius: 20rpx;
  background-color: #fff;
}

.lottery-task .task-title {
  display: flex;
  justify-content: center;
  padding-top: 56rpx;
}

.lottery-task .task-title image {
  width: 428rpx;
  height: 46rpx;
}

.lottery-task .task-content {
  padding-top: 26rpx;
}

.lottery-task .task-item {
  position: relative;
  width: 658rpx;
  height: 134rpx;
  margin-top: 20rpx;
}

.lottery-task .task-item .item-bg {
  width: 658rpx;
  height: 134rpx;
  position: relative;
  z-index: 1;
}

.lottery-task .task-item .item-info {
  position: absolute;
  left: 40rpx;
  top: 0;
  z-index: 3;
  height: 134rpx;
  display: flex;
  flex-direction: column;
}

.lottery-task .task-item .item-title {
  font-size: 40rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #3A3E56;
  line-height: 40rpx;
  margin-top: 30rpx;
}

.lottery-task .task-item .item-subtitle {
  display: flex;
  align-items: center;
  height: 34rpx;
  margin-top: 8rpx;
}

.lottery-task .task-item .item-subtitle image {
  width: 34rpx;
  height: 34rpx;
}

.lottery-task .task-item .item-subtitle text {
  padding-left: 2rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #FE5A7C;
}

.lottery-task .task-item .item-button {
  position: absolute;
  right: 40rpx;
  top: 28rpx;
  z-index: 3;
  width: 220rpx;
  height: 78rpx;
}

.lottery-task .task-item .item-button image {
  width: 100%;
  height: 100%;
}

.lottery-task .task-item .item-button text {
  position: absolute;
  left: 0;
  top: 12rpx;
  z-index: 3;
  width: 100%;
  display: flex;
  justify-content: center;
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: 600;
  color: #C67013;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.lottery-task .task-item .item-button .disabled {
  color: #999;
}

.lottery-task .task-item .item-button .button {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 5;
}

.lottery-task .task-item .item-button .lottery-login {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  overflow: hidden;
  opacity: .2;
  z-index: 6;
}

.dialog {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 99;
  background: rgba(0, 0, 0, 0.5);
}

.dialog-info {
  width: 390rpx;
  position: absolute;
  left: 50%;
  top: 40%;
  transform: translate(-50%, -50%);
  background: #fff;
  border-radius: 20rpx;
  padding: 48rpx 40rpx;
  display: flex;
  justify-content: center;
  flex-direction: column;
}

.dialog-title {
  font-family: PingFang SC;
  font-style: normal;
  font-weight: 500;
  font-size: 36rpx;
  line-height: 42rpx;
  text-align: center;
  color: #222222;
  margin-bottom: 16rpx;
}

.dialog-subtitle {
  font-family: PingFang SC;
  font-style: normal;
  text-align: center;
  margin-bottom: 16rpx;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #4C494A;
}

.dialog-icon {
  height: 146rpx;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.dialog-icon image {
  width: 146rpx;
}

.dialog-btn {
  position: relative;
  height: 88rpx;
  width: 100%;
  margin-top: 32rpx;
}

.dialog-btn .text {
  background: #FFE120;
  border-radius: 44rpx;
  height: 88rpx;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  font-family: PingFang SC;
  font-style: normal;
  font-weight: 500;
  font-size: 32rpx;
  color: #222222;
}

.dialog-close {
  width: 100rpx;
  height: 100rpx;
  position: absolute;
  left: 50%;
  bottom: -145rpx;
  margin-left: -50rpx;
}

.dialog-close image {
  width: 100%;
  height: 100%;
}

.dialog .lottery-login {
  position: absolute;
  left: 0;
  top: 0;
  z-index: 5;
  width: 100%;
  height: 100%;
  overflow: hidden;
  opacity: .2;
}

.dialogRecord {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 99;
  background: rgba(0, 0, 0, 0.5);
}

.dialogRecord-info {
  width: 580rpx;
  position: absolute;
  left: 50%;
  top: 40%;
  transform: translate(-50%, -50%);
  background: #fff;
  border-radius: 20rpx;
  padding: 48rpx 40rpx 80rpx;
  display: flex;
  justify-content: center;
  flex-direction: column;
}

.dialogRecord-title {
  font-family: PingFang SC;
  font-style: normal;
  font-weight: 500;
  font-size: 36rpx;
  line-height: 42rpx;
  text-align: center;
  color: #222222;
}

.dialogRecord-scroll {
  max-height: 660rpx;
  overflow-x: hidden;
  overflow-y: auto;
}

.dialogRecord-item {
  margin-top: 36rpx;
  display: flex;
  align-items: center;
}

.dialogRecord-item .item-icon {
  display: block;
  width: 96rpx;
  height: 96rpx;
}

.dialogRecord-item .item-info {
  flex: 1;
  min-width: 0;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  padding-top: 6rpx;
  padding-left: 14rpx;
}

.dialogRecord-item .item-title {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #333333;
  line-height: 42rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.dialogRecord-item .item-date {
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #999999;
  line-height: 32rpx;
}

.dialogRecord-item .item-btn {
  width: 100rpx;
  height: 50rpx;
  line-height: 50rpx;
  background: #FFE120;
  text-align: center;
  border-radius: 60rpx;
  font-size: 24rpx;
}

.dialogRecord-item .item-btn text {
  font-size: 24rpx;
  color: #333;
}

.dialogRecord-item .item-tips {
  width: 140rpx;
  font-size: 24rpx;
  color: #999;
}

.dialogRecord-empty {
  display: flex;
  justify-content: center;
  font-size: 24rpx;
  color: #787878;
  padding: 100rpx 0;
}

.dialogRecord-close {
  width: 100rpx;
  height: 100rpx;
  position: absolute;
  left: 50%;
  bottom: -145rpx;
  margin-left: -50rpx;
}

.dialogRecord-close image {
  width: 100%;
  height: 100%;
}

.dialogRule {
  padding-top: 30rpx;
}

.dialogRule image {
  width: 100%;
  display: block;
}
